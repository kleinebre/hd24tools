  Function {song(hd24song* store)} {open return_type void
  } {
    code {cout << "Setting audio store, audio=" << audio << ", store=" << store << endl;
audio->audiostore((AudioStorage*)store);
cout << "Setting audio store done." << endl;
if (store!=NULL)
{
	this->enable();
