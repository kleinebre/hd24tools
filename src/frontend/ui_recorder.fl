			posoffset++;
			if ((frpos+i)==(int64_t)(mysong->getlocatepos(hd24song::LOCATEPOS_LOOPEND)))
			{
				frpos=mysong->getlocatepos(hd24song::LOCATEPOS_LOOPSTART);
