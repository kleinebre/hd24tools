	mixerchannel[tracknum]=mixer->parentui()->mixerchannel[tracknum]->control;
}

unsigned int i;
int64_t intsamval=0;
mysong->golocatepos(frame_time);
int jumpedloop=0;
if (transport_state!=JackTransportStopped)
{
	int64_t frpos=frame_time;
	int64_t posoffset=0;
	for (i=0;i<nframes;i++)
	{
		if ( mythis->loopmode()==1 )
